{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "never"],
        "quotes": ["error", "single"],
        "no-unused-expressions": [
            "error",
            {
                "allowTaggedTemplates": true
            }
        ],
        "indent": ["error", 4],
        "brace-style": 0,
        "function-paren-newline": 0,
        "no-console": "warn"
    }
    
}
